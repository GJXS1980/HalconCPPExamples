<procedure name="rectify_image">
<interface>
<io>
<par name="Image" base_type="iconic" dimension="0"/>
</io>
<oo>
<par name="ImageRectified" base_type="iconic" dimension="0"/>
</oo>
<ic>
<par name="RectificationData" base_type="ctrl" dimension="0"/>
<par name="HandEyeCalibData" base_type="ctrl" dimension="0"/>
</ic>
</interface>
<body>
<l>read_dict_tuple (RectificationData, 'RectifyImage', RectifyImage)</l>
<l>if (RectifyImage == 'true')</l>
<l>    get_dict_object (RectificationMap, RectificationData, 'RectificationMap')</l>
<l>    map_image (Image, RectificationMap, ImageRectified)</l>
<l>else</l>
<l>    copy_image (Image, ImageRectified)</l>
<l>endif</l>
<l>return ()</l>
</body>
